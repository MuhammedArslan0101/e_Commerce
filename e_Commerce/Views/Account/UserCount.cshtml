@using e_Commerce.Identity
@model IEnumerable<ApplicationUser>

<div class="col-lg-3 col-xs-6">
    <!-- small box -->
    <div class="small-box bg-teal">
        <div class="inner">
            <h3>@Model.Count()</h3>
            <p>Users Count</p>
        </div>
        <div class="icon">
            <i class="glyphicon glyphicon-user" style="padding-top:13px;"></i>
        </div>
        <a href="/Account/UserList" class="small-box-footer" target="_blank">More info <i class="fa fa-arrow-circle-right"></i></a>
    </div>
</div><!-- ./col -->
